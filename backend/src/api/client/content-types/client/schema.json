{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Client"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "company_name": {
      "type": "string"
    },
    "representative": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "website_URL": {
      "type": "string"
    },
    "testimonial": {
      "type": "richtext"
    },
    "company_logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "project_client"
    },
    "logo_width": {
      "type": "integer"
    },
    "logo_height": {
      "type": "integer"
    }
  }
}
